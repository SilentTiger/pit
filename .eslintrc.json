{
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "jest": true
  },
  "extends": [
    "alloy",
    "alloy/typescript"
  ],
  "root": true,
  "rules": {
    // 自定义你的规则
    "max-params": ["error", 6],
    "@typescript-eslint/prefer-for-of": "off",
    "@typescript-eslint/explicit-member-accessibility": ["error", {
      "accessibility": "explicit",
      "overrides": {
        "accessors": "no-public",
        "constructors": "no-public",
        "methods": "explicit",
        "properties": "explicit",
        "parameterProperties": "explicit"
      }
    }]
  }
}

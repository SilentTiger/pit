{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": false,
        "member-ordering": [true, {"order": "fields-first"}],
        "no-console": false,
        "completed-docs": [true, "enums", "functions", "methods"],
        "object-literal-sort-keys": false,
    },
    "rulesDirectory": []
}